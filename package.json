{
  "name": "react-tutorial",
  "version": "0.0.0",
  "description": "ES6 version of code from the React tutorial.",
  "main": "server.js",
  "dependencies": {
    "body-parser": "^1.4.3",
    "express": "^4.4.5",
    "express-handlebars": "^3.0.0",
    "jquery": "^2.2.3",
    "react": "^15.0.1",
    "react-dom": "^15.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "nodemon": "^1.9.1",
    "webpack": "^1.12.15"
  },
  "scripts": {
    "build": "babel src -d dist && babel server.jsx -o server.js && webpack dist/app.js public/scripts/bundle.js",
    "build-client": "babel src -d dist",
    "build-server": "babel server.jsx -o server.js",
    "build-scripts": "webpack dist/app.js public/scripts/bundle.js",
    "watch-client": "babel src -w -d dist",
    "watch-server": "babel server.jsx -w -o server.js",
    "watch-scripts": "webpack dist/app.js -w public/scripts/bundle.js",
    "watch-start": "nodemon",
    "reset-db": "node seed.js",
    "start": "node server.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/reactjs/react-tutorial.git"
  },
  "keywords": [
    "react",
    "tutorial",
    "comment",
    "example"
  ],
  "author": "petehunt",
  "bugs": {
    "url": "https://github.com/reactjs/react-tutorial/issues"
  },
  "homepage": "https://github.com/reactjs/react-tutorial",
  "engines": {
    "node": "0.12.x"
  }
}
