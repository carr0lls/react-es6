{
  "name": "react-tutorial",
  "version": "0.0.0",
  "description": "ES6 version of code from the React tutorial.",
  "main": "server.bundle.js",
  "dependencies": {
    "body-parser": "^1.4.3",
    "express": "^4.4.5",
    "express-handlebars": "^3.0.0",
    "jquery": "^3.4.1",
    "react": "^15.0.1",
    "react-dom": "^15.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.7",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "nodemon": "^1.9.1",
    "webpack": "^1.12.15",
    "webpack-dev-server": "^1.14.1"
  },
  "scripts": {
    "build": "npm run build:client && npm run build:server",
    "build:client": "webpack",
    "build:server": "webpack --config webpack.server.config.js",
    "watch:client": "webpack -w",
    "watch:server": "webpack -w --config webpack.server.config.js",
    "watch:start": "nodemon",
    "resetdb": "node seed.js",
    "start": "node server.bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/reactjs/react-tutorial.git"
  },
  "keywords": [
    "react",
    "tutorial",
    "comment",
    "example"
  ],
  "author": "petehunt",
  "bugs": {
    "url": "https://github.com/reactjs/react-tutorial/issues"
  },
  "homepage": "https://github.com/reactjs/react-tutorial",
  "engines": {
    "node": "0.12.x"
  }
}
